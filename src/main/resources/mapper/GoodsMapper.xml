<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.manage.demoServer.mapper.GoodsMapper">
    <select id="findById" parameterType="Integer" resultType="com.manage.demoServer.models.GoodsModel">
        SELECT * FROM goods WHERE id = #{id}
    </select>
    <select id="findAll" resultType="com.manage.demoServer.models.GoodsModel">
        SELECT * FROM goods
    </select>
</mapper>